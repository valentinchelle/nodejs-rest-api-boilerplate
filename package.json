    "mongodb-memory-server": "^6.2.4",
    "mongoose": "^5.3.11",
    "mongoose-unique-validator": "^2.0.3",
    "node-sass": "^4.13.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",

{
  "name": "nodejs-rest-api-boilerplate",
  "version": "1.0.0",
  "description": "NodeJS RestAPI",
  "main": "server.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
